<div class="col-lg-6">

  <h1>Listing event_attributes</h1>

  <table class="table table-bordered">
    <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @event_attributes.each do |event_attribute| %>
        <tr>
          <td><%= link_to 'Show', event_attribute %></td>
          <td><%= link_to 'Edit', edit_event_attribute_path(event_attribute) %></td>
          <td><%= link_to 'Destroy', event_attribute, method: :delete, data: {confirm: 'Are you sure?' } %></td>
        </tr>
    <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Event attribute', new_event_attribute_path %>
</div>